  <exec_depend>quad_mgs</exec_depend>
  <exec_depend>quad_utils</exec_depend>
  <exec_depend>quad_global_body_planner</exec_depend>
  <exec_depend>quad_local_planner</exec_depend>
  <exec_depend>quad_nmpc_controller</exec_depend>
  <exec_depend>quad_gazebo</exec_depend>
  <exec_depend>quad_a1_description</exec_depend>
  
  
  <export>
    <build_type>ament_cmake</build_type>
  </export>

